/ {
	model = "Variscite i.MX7 Dual VAR-SOM-MX7";
	compatible = "variscite,imx7d-var-som", "fsl,imx7d";

	cpus {
		cpu0: cpu@0 {
			operating-points = <
				/* KHz	uV */
				996000	1075000
				792000	975000
			>;
		};
	};

	memory {
		reg = <0x80000000 0x80000000>;
	};

